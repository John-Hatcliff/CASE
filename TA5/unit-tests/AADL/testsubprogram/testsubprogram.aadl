package testsubprogram
public
	subprogram service_it
	end service_it;
	
	thread source_t
	end source_t;

	thread implementation source_t.impl
		calls {
			call_server: subprogram service_it;
		};
	end source_t.impl;
	
	thread destination_t
		features
			service: server subprogram service_it;
	end destination_t;

	thread implementation destination_t.impl
	end destination_t.impl;
	
	processor proc
	end proc;

	processor implementation proc.impl
	end processor implementation proc.impl

	process top_process
	end top_process;

	process implementation top_process.impl
		subcomponents
			source_inst: thread source_t.impl;
			destination_inst: thread destination_t.impl;
		properties
			Actual_Subprogram_Call => 
				(reference (destination_inst.service)) applies to source_inst.call_server;
	end top_process.impl;
	
	
	system top
	end top;
	
	system implementation top.impl
		subcomponents
			proc : processor proc.impl
			testsubprogram: process top_process.impl;
		properties
			Actual_Processor_Binding => (reference (proc)) applies to testsubprogram; 
	end top.impl;

end testsubprogram;
